<table class="table table-striped">
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Action</th>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <td><%= user.username %></td>
    <td><%= user.email %></td>
    <td>
      <%= link_to 'delete', admin_user_path(user), :confirm => "Are you sure you would like to delete this user?", :class => 'delete', :method => :delete %>
    </td>
  </tr>
  <% end %>
</table>
